  <DataSets>
    <DataSet Name="UsersDataSet">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UsersCount">
            <Value>=Parameters!UsersCount.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @N as int = @UsersCount;
WITH NRecs
AS (
	SELECT 1 AS UserKey
	
	UNION ALL
	
	SELECT UserKey + 1
	FROM NRecs
	WHERE UserKey&lt; @N
	)
SELECT *,'Corporate user #'+cast(UserKey as varchar(max)) as UserName
FROM NRecs
OPTION (Maxrecursion 32767)</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="SecondaryDataSQL">
          <Value>="("+CStr(Fields!UserKey.Value)+",'"+CStr(Fields!UserName.Value)+"')"</Value>
        </Field>
        <Field Name="UserKey">
          <DataField>UserKey</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="UserName">
          <DataField>UserName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="MainDataSet">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@RecordsCount">
            <Value>=Parameters!RecordsCount.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@UsersCount">
            <Value>=Parameters!UsersCount.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SecondaryDataSQL">
            <Value>=Join(Parameters!SecondaryDataSQL.Value,",")</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @N AS INT =@RecordsCount;

WITH NRecs
AS (
	SELECT 1 AS MainKey
	
	UNION ALL
	
	SELECT MainKey + 1
	FROM NRecs
	WHERE MainKey &lt; @N
	)
	,source
AS (
	SELECT MainKey
		,'main data ' + cast(MainKey AS VARCHAR(max)) AS MainData
		,MainKey % @UsersCount  AS UserKey
		,cast(NULL AS VARCHAR(max)) AS UserName
	FROM NRecs
	)
	,create_source_table_nullable
AS (
	SELECT TOP 0 B.*
	FROM source AS A
	LEFT JOIN source AS B ON 1 = 0
	
	UNION ALL
	
	SELECT *
	FROM source
	)
SELECT *
INTO #results_temporarty_table
FROM create_source_table_nullable
OPTION (MAXRECURSION 32767)

IF (@SecondaryDataSQL IS NOT NULL)
BEGIN
	DECLARE @sql AS NVARCHAR(max) = '
	WITH S
AS (
	SELECT *
	FROM (
		VALUES ' + @SecondaryDataSQL + '
		) AS A(UserKey, UserName)
	)
UPDATE #results_temporarty_table
SET UserName = S.UserName
FROM #results_temporarty_table U
	,S
WHERE U.UserKey = S.UserKey '

	EXEC (@sql)
END

SELECT *
FROM #results_temporarty_table</CommandText>
      </Query>
      <Fields>
        <Field Name="MainKey">
          <DataField>MainKey</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="MainData">
          <DataField>MainData</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="UserKey">
          <DataField>UserKey</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="UserName">
          <DataField>UserName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
